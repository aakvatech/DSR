 "client_script": "frappe.web_form.on('fuel_station', (field, value) => {\r\n\tfilterFuelItem(value);\r\n});\r\n\r\nfunction filterFuelItem(fuel_station) {\r\n    var myurl = 'api/resource/Fuel%20Item?filters=[[\"Fuel%20Item\",\"fuel_station\",\"=\",\"' + fuel_station + '\"]]';\r\n\t$.ajax({\r\n\t\ttype: 'GET', \r\n\t\turl: myurl,\r\n\t\tsuccess: function(result) {\r\n\t\t\tvar options = [];\r\n\t\t\tfor (var i = 0; i < result.data.length; i++) {\r\n\t\t\t\toptions.push({\r\n\t\t\t\t\t'label': result.data[i].name,\r\n\t\t\t\t\t'value': result.data[i].name\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t\tvar field = frappe.web_form.get_field(\"fuel_item\");\r\n\t\t\tfield._data = options;\r\n\t\t\tfield.refresh();\r\n\t\t}\r\n\t});\r\n};\t\r\n",
